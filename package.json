{
  "name": "handbrake-queue",
  "version": "1.1.0",
  "description": "A utility to create queue files for Handbrake encoder.",
  "license": "MIT",
  "main": "electron.js",
  "scripts": {
    "start": "electron.cmd compiled/electron.js",
    "compile": "tsc",
    "stylus": "stylus -w stylus/app.styl -o compiled/app.css",
    "lint": "tslint ./scripts/**/*.ts -t verbose",
    "package": "tsc && electron-packager . HandbrakeQueue --package-manager yarn --out \"d:/Portable software/\" --overwrite --icon=./icon.ico --deref-symlinks false --verbose"
  },
  "repository": "https://github.com/M0ns1gn0r/HandbrakeQueue",
  "author": "Monsignor",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "core-js": "^2.4.1",
    "fileapi": "^2.0.21",
    "jquery": "^3.1.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.5.6",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4",
    "video.js": "^6.6.0"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/node": "^6.0.53",
    "@types/video.js": "^6.2.3",
    "electron": "1.6.16",
    "electron-packager": "^11.0.1",
    "stylus": "^0.54.5",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  }
}
